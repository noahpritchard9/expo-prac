generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  name     String
  email    String @unique
  password String
  age      Int
  role     Role   @default(User)
}

enum Role {
  User
  Dev
  ShopOwner
}
